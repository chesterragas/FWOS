<head>
    <meta charset="utf-8" />
    <title>Register | Work Slip Order</title>
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />

    <!-- ================== BEGIN BASE CSS STYLE ================== -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700" rel="stylesheet" />
    <link href="~/Content/assets/plugins/jquery-ui/jquery-ui.min.css" rel="stylesheet" />
    <link href="~/Content/assets/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/assets/plugins/font-awesome/css/all.min.css" rel="stylesheet" />
    <link href="~/Content/assets/plugins/animate/animate.min.css" rel="stylesheet" />
    <link href="~/Content/assets/css/default/style.min.css" rel="stylesheet" />
    <link href="~/Content/assets/css/default/style-responsive.min.css" rel="stylesheet" />
    <link href="~/Content/assets/css/default/theme/default.css" rel="stylesheet" id="theme" />
    <!-- ================== END BASE CSS STYLE ================== -->
    <!-- ================== BEGIN BASE JS ================== -->
    <script src="~/Content/assets/plugins/pace/pace.min.js"></script>
    <!-- ================== END BASE JS ================== -->
</head>

<body class="pace-top bg-white">
    <!-- begin #page-loader -->
    <div id="page-loader" class="fade show"><span class="spinner"></span></div>
    <!-- end #page-loader -->
    <!-- begin #page-container -->
    <div id="page-container" class="fade">
        <!-- begin register -->
        <div class="register register-with-news-feed">
            <!-- begin news-feed -->
            <div class="news-feed">
                <div class="news-image"
                     style="background-image: url(/Content/assets/img/login-bg/login-bg-9.jpg)"></div>

            </div>
            <!-- end news-feed -->
            <!-- begin right-content -->
            <div class="right-content">
                <!-- begin register-header -->
                <h1 class="register-header">
                    <small>Register your Account</small>
                </h1>
                <!-- end register-header -->
                <!-- begin register-content -->
                <div class="register-content">
                    <form action="./dashboard.html" method="GET" class="margin-bottom-0" id="RegisterForm">
                        <label class="control-label">Name <span class="text-danger">*</span></label>
                        <div class="row row-space-10">
                            <div class="col-md-6 m-b-15">
                                <input type="text" class="form-control" name="FirstName" placeholder="First name" required />
                            </div>
                            <div class="col-md-6 m-b-15">
                                <input type="text" class="form-control" name="LastName" placeholder="Last name" required />
                            </div>
                        </div>
                        <label class="control-label">Email <span class="text-danger">*</span></label>
                        <div class="row m-b-15">
                            <div class="col-md-12">
                                <input type="email" class="form-control" name="Email" placeholder="Email address" required />
                            </div>
                        </div>

                        <label class="control-label">Password <span class="text-danger">*</span></label>
                        <div class="row m-b-15">
                            <div class="col-md-12">
                                <input type="password" class="form-control" name="Password" placeholder="Password" required />
                            </div>
                        </div>

                        <label class="control-label">Employee No <span class="text-danger">*</span></label>
                        <div class="row m-b-15">
                            <div class="col-md-12">
                                <input type="text" class="form-control" name="EmployeeNo" placeholder="Employee No" required />
                            </div>
                        </div>

                        <label class="control-label">Mobile No <span class="text-danger">*</span></label>
                        <div class="row m-b-15">
                            <div class="col-md-12">
                                <input type="text" class="form-control" name="MobileNo" placeholder="Mobile No" required />
                            </div>
                        </div>

                        <label class="control-label">Local No <span class="text-danger">*</span></label>
                        <div class="row m-b-15">
                            <div class="col-md-12">
                                <input type="text" class="form-control" name="LocalNo" placeholder="Local No" required />
                            </div>
                        </div>

                        <label class="control-label">Division <span class="text-danger">*</span></label>
                        <div class="row m-b-15">
                            <div class="col-md-12">
                                <select class="select2 form-control" name="DivisionID" id="DivisionID" required></select>
                            </div>
                        </div>

                        <label class="control-label">Department <span class="text-danger">*</span></label>
                        <div class="row m-b-15">
                            <div class="col-md-12">
                                <select class="select2 form-control" name="DepartmentID" id="DepartmentID" required></select>
                            </div>
                        </div>

                        <label class="control-label">Section <span class="text-danger">*</span></label>
                        <div class="row m-b-15">
                            <div class="col-md-12">
                                <select class="select2 form-control" name="SectionID" id="SectionID" required></select>
                            </div>
                        </div>


                        <div class="register-buttons  mb-2">
                            <button type="submit" class="btn btn-primary btn-block btn-lg">Register</button>
                        </div>

                        <div class="login-buttons">
                            <a href="./index.html" class="btn btn-success btn-block btn-lg">Return to Login</a>
                        </div>
                    </form>
                </div>
                <!-- end register-content -->
            </div>
            <!-- end right-content -->
        </div>
        <!-- end register -->
    </div>
    <!-- end page container -->
    <!-- ================== BEGIN BASE JS ================== -->
    <script src="~/Content/assets/plugins/jquery/jquery-3.3.1.min.js"></script>
    <script src="~/Content/assets/plugins/jquery-ui/jquery-ui.min.js"></script>
    <script src="~/Content/assets/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!--[if lt IE 9]>
        <script src="~/Content/assets/crossbrowserjs/html5shiv.js"></script>
        <script src="~/Content/assets/crossbrowserjs/respond.min.js"></script>
        <script src="~/Content/assets/crossbrowserjs/excanvas.min.js"></script>
    <![endif]-->
    <script src="~/Content/assets/plugins/slimscroll/jquery.slimscroll.min.js"></script>
    <script src="~/Content/assets/plugins/js-cookie/js.cookie.js"></script>
    <script src="~/Content/assets/js/theme/default.min.js"></script>
    <script src="~/Content/assets/js/apps.min.js"></script>

    <!-- ================== END BASE JS ================== -->

    <script>
        $(document).ready(function () {
            App.init();
        });
    </script>
    <script src="~/Scripts/helper.js"></script>
    <script src="~/Scripts/Custom/Register.js"></script>
</body>

</html>